<h1>Recipe name</h1>
<ul>
  <% @ingredients.each do |ingredient|%>
    <li><%= ingredient %></li>
    <% end %>
</ul>

<ol>
  <% @directions.each do |ingredient|%>
    <li><%= ingredient %></li>
    <% end %>
</ol>

<h1>Reviews:</h1>
  <ul>
    <% @reviews.each do |review| %>
    <li><%= review[:username] %> ---- <br>Reviews: <%=review[:rating]%> stars-----<br> <%=review[:review]%></li>
    <% end %>
  </ul>
